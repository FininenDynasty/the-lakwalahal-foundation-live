{
  "version": 2,
  "name": "lakwalahal-foundation",
  "builds": [
    {
      "src": "frontend/build/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "frontend/build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}